<launch>
<!-- Start tilt joint controller -->
    <rosparam file="$(find elir_actuation)/config/claws/claw_joints.yaml" command="load"/>
    <node name="b_dual_motor_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port pan_tilt_port
                  joint_claw_traction_f1
		      joint_claw_traction_f2
                "
          output="screen"/>

<!-- Start the meta controller -->
    <rosparam file="$(find elir_actuation)/config/claws/b_claw_controller.yaml" command="load"/>
    <node name="b_controller_spawner_meta" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --type=meta
		      f_claw_trajectory_controller
                  joint_claw_traction_f1
		      joint_claw_traction_f2
                "
          output="screen"/>
</launch>
